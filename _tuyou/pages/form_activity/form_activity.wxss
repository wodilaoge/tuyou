.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}
.icon{
  position: relative;
  width: 45rpx;
  height: 45rpx;
}
.icon-sub{
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}
.cu-form-group2 {
	background-color: var(--white);
	padding: 30rpx;
	display: flex;
	align-items: center;
	min-height: 100rpx;
	/* justify-content: space-between; */
}
/* 横着的分割View样式 */
/*分割线样式*/
.divLine{
 background:#F0F0F0;
  width: 92%; 
  position: relative;
  top: -20rpx;
  right: -30rpx;
  height: 5rpx;
}
.white{
  height: 40rpx;
  background-color: white;
  margin-top: -10rpx;
}
.input{
  width: 600rpx;
  right: -20rpx;
  position: relative;
  	box-sizing: border-box; 
}
.checkicon{
  transform: scale(0.5,0.5);
}
.margin-top2{
  margin-top: 20rpx;
}
.margin-top3{
  margin-top: 10rpx;
}
.tui-number-group{
 display: table;
 table-layout: fixed;
 width: 220rpx;
 text-align: center;
 border-radius: 6px;
 border: 1px solid #bbb;
 overflow: hidden;
}
.tui-number-cell{
 display: table-cell;
 line-height: 0.8;
 border-radius: 0;
}
button::after{
 border-bottom: none;
 border-top: none;
 border-radius: 0;
}