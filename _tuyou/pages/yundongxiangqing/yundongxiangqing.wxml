创建时间：20200416
创建内容：运动详情-->

<view class="top">
  <cu-custom isBack="{{true}}" bgColor="bg-blue">
    <view slot="content">运动详情</view>
  </cu-custom>
</view>
<swiper class="screen-swiper round-dot" indicator-dots="true" circular="true" autoplay="true" interval="5000" duration="500">
  <swiper-item wx:for="{{swiperList}}" wx:key>
    <image src="{{item.url}}" mode="aspectFill" wx:if="{{item.type=='image'}}"></image>
    <video src="{{item.url}}" autoplay loop muted show-play-btn="{{false}}" controls="{{false}}" objectFit="cover" wx:if="{{item.type=='video'}}"></video>
  </swiper-item>
</swiper>

<scroll-view scroll-x class="bg-white nav text-center">
  <view class="cu-item {{0==TabCur?'text-bold text-blue cur':''}}" bindtap="tabSelect" data-id="0"> 详情</view>
  <view class="cu-item {{1==TabCur?'text-bold text-blue cur':''}}" bindtap="tabSelect" data-id="1"> 报名 </view>
  <view class="cu-item {{2==TabCur?'text-bold text-blue cur':''}}" bindtap="tabSelect" data-id="2"> 视频</view>
  <view class="cu-item {{3==TabCur?'text-bold text-blue cur':''}}" bindtap="tabSelect" data-id="3"> 照片 </view>
  <view class="cu-item {{4==TabCur?'text-bold text-blue cur':''}}" bindtap="tabSelect" data-id="4"> 排名 </view>
  <view class="cu-item {{5==TabCur?'text-bold text-blue cur':''}}" bindtap="tabSelect" data-id="5"> 数据</view>
  <view class="cu-item {{6==TabCur?'text-bold text-blue cur':''}}" bindtap="tabSelect" data-id="6"> 资讯</view>
</scroll-view>

<block wx:if="{{TabCur==0}}">
  <view class="cu-bar bg-white solid-bottom margin-top-sm">
    <image class="cu-avatar round my" src="{{detail.createrhead}}" mode="heightFix"></image>
    <text style="margin-left:-100rpx">三分赛</text>
    <text class=" title-time margin-right">1 天 01: 02 </text>
  </view>

  <view style="margin-bottom:20rpx;">
    <view class="cu-card article no-card" style="margin-top:-20rpx;">
      <view class="cu-item bg-white">
        <view class="shadow" style="background-color:#F8F9FD;">
          <view class="yundong-content">
            <view class="desc padding-xs">
              <view class="title-limit">{{detail.actname}}</view>
              <view class="text-content">
                <image class="icon-size" src="/img/icon_location.png" mode="heightFix"></image>
                <text decode="{{true}}">\t{{detail.city}}{{detail.address}}</text>
              </view>
              <view class="text-content">
                <image class="icon-size"></image>
                <text decode="{{true}}">\t{{detail.venue}}</text>
              </view>
              <view class="time-text-content">
                <image class="icon-size" src="/img/icon_time.png" mode="heightFix"></image>
                3月11日（周三）13:00开赛
              </view>
            </view>
            <view>
              <image class="icon_add" src="/img/icon_add.png" mode="aspectFill" style="margin-top:52rpx;margin-right:83rpx;">
              </image>
              <view class="text-blue margin-top-xs" style="font-size: 24rpx;margin-left:-8rpx;">1.1Km</view>
            </view>
          </view>
        </view>
        <view class="yundong-content padding-xs" style="margin-bottom:18rpx;">
          <image class="icon-size" style="margin-top:28rpx;" src="/img/icon_title.png" mode="heightFix"></image>
          <text class="title-text-content" style="margin-top:28rpx;" decode="{{true}}">{{slogan}}</text>
          <button class="cu-btn line-orange round btn-guanzhu">关注</button>
        </view>

      </view>
      <view class="bg-white">
        <view class="yibaoming-margin-top margin-left ">
          <text class="yibaoming-text-content">已报名</text>
          <text class="yibaoming-text-content" style="color:var(--blue)">21/{{detail.signupmax}}</text>
          <text class="yibaoming-text-content">人</text>
          <button class="cu-btn bg-orange round btn-baomingtiaozhan margin-top">报名挑战</button>
        </view>
        <view class="yundong-content bg-white">
          <view class="padding-sm">
            <view class="cu-avatar round lg margin-sm" wx:for="{{21}}" style="background-image:url(https://ossweb-img.qq.com/images/lol/web201310/skin/big1000{{(index+1)%9}}.jpg);">
            </view>
          </view>
        </view>
      </view>
    </view>

  </view>
  <view class="bg-white padding">
    <view class="cu-bar bg-white ">
      <view class="cu-bar-title">活动介绍</view>
    </view>
    <view class="text-guize " style="width:625rpx;">
      <wxparse data="{{ detail.imagetext }}"></wxparse>
      <view class="margin-top">
        <text class="cuIcon-attention lg text-gray">\t23456</text>
        <text class="cuIcon-appreciate lg text-gray text-right" decode="{{true}}">\t赞&emsp;{{likecount}}</text>
      </view>
    </view>
  </view>
  <view class="bg-white padding margin-top">
    <view class="cu-bar bg-white ">
      <view class="cu-bar-title">奖品设置</view>
    </view>
    <image src="/img/jiangpin.png" mode="scaleToFill" style="width:700rpx;height:399rpx;"></image>
  </view>
  <view class="bg-white padding margin-top">
    <view class="cu-bar bg-white ">
      <view class="cu-bar-title">赞助商</view>
    </view>
    <image src="/img/zanzhushang.png" mode="scaleToFill" style="width:700rpx;height:200rpx;"></image>
  </view>

  <block>
    <view class="huodong-margin-bottom">
      <!-- 评论 -->
      <view class="cu-bar bg-white solid-bottom huodong-margin-top">
        <view class='action'>
          <text class='cu-bar-title'>评论{{comment_detail.length}}</text>
        </view>
      </view>

      <view class="cu-card case no-card bg-white">
        <view class="cu-item shadow">
          <view class="cu-list menu-avatar">
            <view class="cu-item" wx:for="{{comment_detail}}" wx:for-index="index">
              <view class="cu-avatar round lg" style="background-image:url({{item.createrHead}});"></view>
              <view class="content flex-sub">
                <view class=" flex justify-between">
                  <view>
                    <text class="card-text-grey" decode="{{true}}">{{item.createrAlias}}&emsp;</text>
                    <button class="cu-btn btn-hot bg-orange disabled" wx:if="{{item.hot}}">热门</button>
                  </view>
                  <view class="text-gray text-sm">
                    <text class="{{false==item.ifzan?'cuIcon-appreciate':'cuIcon-appreciatefill'}} margin-lr-xs line" bindtap="zan_list" data-objid="{{item.id}}" data-ifzan="{{item.ifzan}}" data-index='{{index}}'></text> {{item.praiseCnt}}
                    <text class="cuIcon-appreciate margin-lr-xs "></text> {{item.degradeCnt}}
                  </view>
                </view>
                <view bindtap="chakanhuifu" data-id="{{item.id}}">
                  <view class="card-text-content">
                    {{item.comment}}
                  </view>
                  <view class="card-text-content-time">
                    {{item.strCreatetime}} {{item.replyCnt}} 回复
                  </view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </block>
  <view>
    <view class="cu-bar input foot padding">
      <input class="solid-bottom" focus="{{false}}" maxlength="300" cursor-spacing="10" placeholder='来说点什么吧！' bindtap="pinluntiaozhuan"></input>
      <view class="action">
        <image class="img-icon" mode="aspectFit" src="/img/icon_talk.png" bindtap="pinluntiaozhuan"></image>
      </view>
      <view class="action">
        <image class="img-icon" mode="aspectFit" style="margin-right:10rpx" src="{{false==ifzan?'/img/icon_fabulous.png':'/img/icon_fabulous_z.png'}}" bindtap="zan"></image>
      </view>
      <view class="action">
        <image class="img-icon" mode="aspectFit" src="/img/icon_share.png"></image>
      </view>
    </view>
  </view>
</block>


